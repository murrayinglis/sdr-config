<?xml version="1.0"?>
<root>
    <device>
        <name>USRP Device</name>
        <IP>192.168.10.2</IP>
    </device>
    <config>
        <TX_SUBDEV>A:0</TX_SUBDEV>
        <RX_SUBDEV>A:0</RX_SUBDEV>
        <TX_ANTENNA>TX/RX</TX_ANTENNA>
        <RX_ANTENNA>RX2</RX_ANTENNA>
        <WAVEFORM_FILE>sweep.csv</WAVEFORM_FILE>
    </config>
    <frequency>
        <TX_FREQ>2700e6</TX_FREQ>
        <RX_FREQ>2700e6</RX_FREQ>
        <TX_RATE>25e6</TX_RATE>
        <RX_RATE>25e6</RX_RATE>
        <TX_BW>30e6</TX_BW>
        <RX_BW>30e6</RX_BW>
        <TX_GAIN>0</TX_GAIN>
        <RX_GAIN>0</RX_GAIN>
    </frequency>
    <test>
        <TEST_TYPE>TX_SINGLE_FREQ</TEST_TYPE>
        <CONT>false</CONT>
        <NUM_SAMPLES>500000</NUM_SAMPLES>
        <RX_START_TIME>2</RX_START_TIME>
        <TX_START_TIME>2</TX_START_TIME>
    </test>
    <radar>
        <RADAR_TYPE>SFCW</RADAR_TYPE>
        
    </radar>
    <options>
        <OUTPUT_FILE></OUTPUT_FILE>
        <VERBOSE>true</VERBOSE>
    </options>
</root>